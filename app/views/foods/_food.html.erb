

<div id="<%= dom_id food %>" >
  <div class="food-item">

    <p>
      <%= food.name.upcase %>
    </p>
    <p><%= food.measurement_unit %> </p>
    <p>$ <%= food.price %> </p>
    <p><%= food.quantity %> </p>
    
    <%= button_to 'destroy', user_food_path(user_id: current_user.id, id: food.id), method: 'delete' %>
  
  </div>
</div>